{
  "name": "bnti",
  "version": "0.1.0",
  "homepage": "http://bnti.s3-website.eu-west-2.amazonaws.com",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "start": "rescripts start",
    "start:prod": "yarn build && serve -s build -l 3000",
    "start-api": "sls offline start",
    "build": "rescripts build",
    "test": "rescripts test --verbose --coverage --no-cache",
    "test:e2e": "start-server-and-test start 3000 cypress:run",
    "test:e2e:interactive": "start-server-and-test start 3000 cypress:open",
    "lint": "eslint ./src && stylelint ./src",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open"
  },
  "dependencies": {
    "aws-amplify": "1.1.19",
    "chance": "1.0.18",
    "history": "4.7.2",
    "polished": "3.0.0",
    "prop-types": "15.6.2",
    "react": "16.8.1",
    "react-dom": "16.8.1",
    "react-helmet": "5.2.0",
    "react-router-dom": "4.3.1",
    "styled-components": "4.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "7.5.0",
    "@commitlint/config-conventional": "7.5.0",
    "@rescripts/cli": "0.0.10",
    "@rescripts/rescript-env": "0.0.5",
    "babel-plugin-jsx-remove-data-test-id": "2.0.0",
    "babel-plugin-styled-components": "1.10.0",
    "cypress": "3.1.5",
    "cypress-testing-library": "2.3.5",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.0.0",
    "eslint-plugin-cypress": "2.2.0",
    "eslint-plugin-jest": "22.2.2",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react-hooks": "1.0.1",
    "husky": "1.3.1",
    "jest-dom": "3.0.2",
    "jest-styled-components": "6.3.1",
    "lint-staged": "8.1.3",
    "prettier": "1.16.4",
    "react-scripts": "2.1.5",
    "react-testing-library": "5.5.4",
    "serve": "10.1.1",
    "serverless": "1.37.0",
    "serverless-functions-base-path": "1.0.28",
    "serverless-offline": "4.2.1",
    "serverless-plugin-optimize": "4.0.2-rc.1",
    "serverless-s3-sync": "1.7.2",
    "start-server-and-test": "1.7.11",
    "stylelint": "9.10.1",
    "stylelint-config-recommended": "2.1.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.5.2",
    "user-event": "1.4.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js",
      "!src/setupTests.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,yml,md,html}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "yarn test && yarn test:e2e"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "rescripts": [
    "env"
  ]
}
